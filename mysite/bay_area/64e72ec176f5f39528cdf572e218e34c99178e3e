 function LazyLoadImg(container) {
 if (!container) return;
 var images = container.getElementsByTagName('IMG');
 var length = images.length;
 for (var i = 0; i < length; i++) {
 var obj = images[i];
 if (obj.attributes.imgurl.value) {
 obj.src = obj.attributes.imgurl.value;
 }
 }
 }
 function LazyLoadImgForSold() { 
 LazyLoadImg(document.getElementById('ctl_main_divMarketStatisticsChart'));
 LazyLoadImg(document.getElementById('nearbyHouses'));
 LazyLoadImg(document.getElementById('neighborhoodChart'));
 LazyLoadImg(document.getElementById('ctl_main_dppNearByHomes')); 
 }
 function LoadJS(url, script) {
 var scriptdom = document.createElement('script');
 scriptdom.type = 'text/javascript';
 scriptdom.loaded = false;
 scriptdom.src = url;
 if (script) {
 //for ie
 var ua = navigator.userAgent.toLowerCase();
 if (!(ua.indexOf("opera") > -1) && ua.indexOf("msie") > -1) {
 scriptdom.onreadystatechange = function () {
 if ((scriptdom.readyState === 'loaded' || scriptdom.readyState === 'complete') && !scriptdom.loaded) {
 script();
 scriptdom.onload = scriptdom.onreadystatechange = null;
 }
 };
 }
 else {
 scriptdom.onload = function () {
 scriptdom.loaded = true;
 script();
 scriptdom.onload = scriptdom.onreadystatechange = null;
 };
 }
 }
 document.getElementsByTagName('head')[0].appendChild(scriptdom);
 }
